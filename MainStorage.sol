contract MainStorage is AccessControlEnumerable, IMainStorage {
    enum Flag {
        nativeToken,
        treasury,
        cashFlow,
        usd
    }

    bytes32 public constant DEPOSITOR_ROLE = keccak256("DEPOSITOR_ROLE");
    bytes32 public constant SIGNER_ROLE = keccak256("SIGNER_ROLE");
    uint256 public constant DIVISOR = 100e18;

    address public nativeToken;
    address public treasury;
    address public cashFlow;
    address public usd;

    mapping(address => uint256[]) public injectedAmountById;
    mapping(address => uint256) public totalInjectedAmount;
    mapping(address => uint256) public cashFlowFee;
    mapping(address => uint256) public fee;
    mapping(address => bool) public hasAccess;

    event UpdateAddress(
        Flag indexed flag,
        address indexed oldAddress,
        address indexed newAddress
    );

    constructor(
        address _owner,
        address _nativeToken,
        address _treasury,
        address _cashflow,
        address _signer,
        address _usd
    ) {
        _grantRole(DEFAULT_ADMIN_ROLE, _owner);
        _updateNativeToken(_nativeToken);
        _updateTreasury(_treasury);
        _updateCashFlow(_cashflow);
        _grantRole(SIGNER_ROLE, _signer);
        _updateUsd(_usd);
    }

    modifier onlyValidAmount(uint256 value) {
        require(value > 0, "Invalid amount");
        _;
    }

    modifier onlyValidContractAddr(address account) {
        require(account.code.length > 0, "Invalid Contract Address");
        _;
    }

    modifier onlyValidAddr(address account) {
        require(account != address(0), "Invalid address");
        _;
    }

    modifier onlyValidEoaAddr(address account) {
        require(
            account != address(0) && account.code.length == 0,
            "Invalid EOA address"
        );
        _;
    }

    function updateCashFlowFee(
        address account,
        uint256 percentage
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(account != address(0), "updateCashFlowFee: Invalid Address");
        require(
            percentage > 0 && percentage <= DIVISOR,
            "updateCashFlowFee: Invalid percentage"
        );
        cashFlowFee[account] = percentage;
    }

    function updateUsd(address _newUsd) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _updateUsd(_newUsd);
    }

    function updateNativeToken(
        address _newNativeToken
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _updateNativeToken(_newNativeToken);
    }

    function updateCashFlow(
        address newCashFlow
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _updateCashFlow(newCashFlow);
    }

    function updateTreasury(
        address newTreasury
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _updateTreasury(newTreasury);
    }

    function updateAccess(
        address[] memory account,
        bool[] memory value
    ) external onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        uint256 length = account.length;
        require(
            length > 0 && length == value.length,
            "updateAccess: Invalid Params"
        );
        for (uint256 i = 0; i < length; i++) {
            require(
                hasAccess[account[i]] != value[i],
                "updateAccess: Already this value applied"
            );
            hasAccess[account[i]] = value[i];
        }
        return true;
    }

    function updateInjectedAmount(
        address account,
        uint256 amount
    ) external onlyValidAmount(amount) returns (bool) {
        require(
            hasAccess[_msgSender()],
            "updateTotalInjectedAmount: No access to update"
        );
        totalInjectedAmount[account] += amount;
        injectedAmountById[account].push(amount);
        return true;
    }

    function updateFee(
        address account,
        uint256 value
    )
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        onlyValidAddr(account)
        onlyValidAmount(value)
    {
        require(value <= DIVISOR, "updateFee: Invalid Divisor");
        fee[account] = value;
    }

    function hasSignerRole(address account) external view returns (bool) {
        return hasRole(SIGNER_ROLE, account);
    }

    function hasDepositorRole(address account) external view returns (bool) {
        return hasRole(DEPOSITOR_ROLE, account);
    }

    function injectedId(address account) external view returns (uint256) {
        return injectedAmountById[account].length;
    }

    function _updateUsd(
        address _newUsd
    ) private onlyValidContractAddr(_newUsd) {
        emit UpdateAddress(Flag.usd, usd, _newUsd);
        usd = _newUsd;
    }

    function _updateNativeToken(
        address _newNativeToken
    ) private onlyValidContractAddr(_newNativeToken) {
        emit UpdateAddress(Flag.nativeToken, nativeToken, _newNativeToken);
        nativeToken = _newNativeToken;
    }

    function _updateCashFlow(
        address _newCashFlow
    ) private onlyValidContractAddr(_newCashFlow) {
        emit UpdateAddress(Flag.cashFlow, cashFlow, _newCashFlow);
        cashFlow = _newCashFlow;
    }

    function _updateTreasury(
        address _newTreasury
    ) private onlyValidAddr(_newTreasury) {
        emit UpdateAddress(Flag.treasury, treasury, _newTreasury);
        treasury = _newTreasury;
    }
}
