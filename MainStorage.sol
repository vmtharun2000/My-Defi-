interface IMainStorage {
    function DIVISOR() external pure returns (uint256);

    function nativeToken() external view returns (address);

    function treasury() external view returns (address);

    function cashFlow() external view returns (address);

    function usd() external view returns (address);

    function hasSignerRole(address account) external view returns (bool);

    function hasDepositorRole(address account) external view returns (bool);

    function cashFlowFee(address account) external view returns (uint256);

    function fee(address account) external view returns (uint256);

    function injectedId(address account) external view returns (uint256);

    function updateInjectedAmount(
        address account,
        uint256 amount
    ) external returns (bool);
}

